{
  "ob": {
    "s": {
      "border_radius": {
        "none": {
          "$type": "borderRadius",
          "$value": "{ob.p.dimension.px.0}"
        },
        "sm": {
          "$type": "borderRadius",
          "$value": "{ob.p.dimension.px.1}"
        },
        "md": {
          "$type": "borderRadius",
          "$value": "{ob.p.dimension.px.2}"
        },
        "lg": {
          "$type": "borderRadius",
          "$value": "{ob.p.dimension.px.4}"
        },
        "rounded": {
          "$type": "borderRadius",
          "$value": "{ob.p.dimension.px.9999}"
        }
      },
      "border_width": {
        "xs": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.1}"
        },
        "sm": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.2}"
        },
        "md": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.3}"
        },
        "lg": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.4}"
        },
        "xl": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.8}"
        },
        "2xl": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.12}"
        },
        "3xl": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.16}"
        },
        "none": {
          "$type": "borderWidth",
          "$value": "{ob.p.dimension.px.0}"
        }
      },
      "outline_offset": {
        "none": {
          "$type": "dimension",
          "$value": "{ob.p.dimension.px.0}",
          "$description": "No outline offset - outline directly touches element boundary"
        },
        "xs": {
          "$type": "dimension",
          "$value": "{ob.p.dimension.px.1}",
          "$description": "Minimal outline offset for tight spacing requirements"
        },
        "sm": {
          "$type": "dimension",
          "$value": "{ob.p.dimension.px.2}",
          "$description": "Small outline offset for standard interactive elements"
        },
        "md": {
          "$type": "dimension",
          "$value": "{ob.p.dimension.px.3}",
          "$description": "Medium outline offset for buttons and larger interactive elements"
        },
        "lg": {
          "$type": "dimension",
          "$value": "{ob.p.dimension.px.4}",
          "$description": "Large outline offset for prominent interactive elements"
        }
      },
      "border": {
        "focus_ring": {
          "inversity_normal": {
            "$type": "border",
            "$value": {
              "color": "{ob.s3.color.interaction.focus_ring.inversity_normal}",
              "width": "{ob.s.border_width.md}",
              "style": "solid"
            },
            "$description": "CSS implementation of focus ring for accessibility compliance. Use with outline property."
          },
          "inversity_flipped": {
            "$type": "border",
            "$value": {
              "color": "{ob.s3.color.interaction.focus_ring.inversity_flipped}",
              "width": "{ob.s.border_width.md}",
              "style": "solid"
            },
            "$description": "CSS implementation of focus ring for accessibility compliance. Use with outline property."
          }
        }
      }
    }
  }
}