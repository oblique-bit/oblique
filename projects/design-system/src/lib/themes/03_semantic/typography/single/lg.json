{
  "ob": {
    "s": {
      "dynamic": {
        "font_size": {
          "xs": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.100} * {ob.g.multiplier.typography.lg}"
          },
          "sm": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.200} * {ob.g.multiplier.typography.lg}"
          },
          "md": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.300} * {ob.g.multiplier.typography.lg}"
          },
          "lg": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.400} * {ob.g.multiplier.typography.lg}"
          },
          "xl": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.500} * {ob.g.multiplier.typography.lg}"
          },
          "2xl": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.600} * {ob.g.multiplier.typography.lg}"
          },
          "3xl": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.700} * {ob.g.multiplier.typography.lg}"
          },
          "4xl": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.800} * {ob.g.multiplier.typography.lg}"
          },
          "5xl": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.900} * {ob.g.multiplier.typography.lg}"
          },
          "base": {
            "$type": "fontSizes",
            "$value": "{ob.p.font_size_rem.300} * {ob.g.multiplier.typography.lg}"
          }
        },
        "lineHeight": {
          "xs": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.300} * {ob.g.multiplier.typography.lg}"
          },
          "sm": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.400} * {ob.g.multiplier.typography.lg}"
          },
          "md": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.500} * {ob.g.multiplier.typography.lg}"
          },
          "lg": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.600} * {ob.g.multiplier.typography.lg}"
          },
          "xl": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.800} * {ob.g.multiplier.typography.lg}"
          },
          "2xl": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.1100} * {ob.g.multiplier.typography.lg}"
          },
          "3xl": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.1300} * {ob.g.multiplier.typography.lg}"
          },
          "4xl": {
            "$type": "lineHeights",
            "$value": "{ob.p.lineHeightRem.1400} * {ob.g.multiplier.typography.lg}"
          }
        },
        "paragraphSpacing": {
          "xs": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.xs} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "sm": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.sm} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "md": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.md} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "lg": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.lg} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "xl": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.xl} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "2xl": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.2xl} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "3xl": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.3xl} * {ob.g.multiplier.typography.lg}) / 2"
          },
          "4xl": {
            "$type": "paragraphSpacing",
            "$value": "({ob.s.dynamic.lineHeight.4xl} * {ob.g.multiplier.typography.lg}) / 2"
          }
        },
        "spacing": {
          "heading": {
            "top": {
              "$type": "spacing",
              "$value": "{ob.s.dimension.static.component_size.none.px}",
              "$description": "Standard spacing above H1 is none to use vertical space."
            },
            "top_after_p": {
              "$type": "spacing",
              "$value": "{ob.s.dimension.static.component_size.container.md.px}",
              "$description": "SPecial spacing above H1 after a paragraph"
            },
            "bottom": {
              "$type": "spacing",
              "$value": "{ob.s.dimension.static.component_size.element.md.px}"
            }
          },
          "paragraph": {
            "bottom": {
              "$type": "spacing",
              "$value": "{ob.s.dimension.static.density.6xl.rem}"
            }
          }
        },
        "font_weight": {
          "regular": {
            "$type": "fontWeights",
            "$value": "{ob.p.font_weight.400}"
          },
          "medium": {
            "$type": "fontWeights",
            "$value": "{ob.p.font_weight.500}"
          },
          "semiBold": {
            "$type": "fontWeights",
            "$value": "{ob.p.font_weight.600}"
          },
          "bold": {
            "$type": "fontWeights",
            "$value": "{ob.p.font_weight.700}"
          },
          "extraBold": {
            "$type": "fontWeights",
            "$value": "{ob.p.font_weight.800}"
          }
        },
        "text_decoration": {
          "link": {
            "emphasis_high": {
              "$type": "textDecoration",
              "$value": "{ob.p.text_decoration.underline}",
              "$description": "Standard text-link has an underline."
            },
            "emphasis_low": {
              "$type": "textDecoration",
              "$value": "{ob.p.text_decoration.none}",
              "$description": "Use where it is obvious that it is link and no need for additional typographical emphasis such as underline."
            }
          }
        },
        "letter_spacing_rem": {
          "superNarrow": {
            "$type": "letterSpacing",
            "$value": "-0.03rem",
            "$description": "For font sizes 4xl, 5xl, 6xl for a more compact visual appearance. \nHas to be convertered to em!"
          },
          "narrower": {
            "$type": "letterSpacing",
            "$value": "-0.01rem",
            "$description": "For font size 3x for a more compact visual appearance.\nHas to be convertered to em!"
          },
          "narrow": {
            "$type": "letterSpacing",
            "$value": "0.005rem",
            "$description": "For font sizes 2xl for a more compact visual appearance.\nHas to be convertered to em!\n"
          },
          "normal": {
            "$type": "letterSpacing",
            "$value": "auto",
            "$description": "Default letter spacing for body and labels.\nHas to be convertered to em!"
          },
          "wide": {
            "$type": "letterSpacing",
            "$value": "0.5%",
            "$description": "For buttons for a more confortable visual appearance\nHas to be convertered to em!"
          }
        },
        "letter_spacing_px": {
          "superNarrow": {
            "$type": "letterSpacing",
            "$value": "{ob.p.letterSpacing_px.neg_150}",
            "$description": "For font sizes 4xl, 5xl, 6xl for a more compact visual appearance. \nHas to be convertered to em!"
          },
          "narrower": {
            "$type": "letterSpacing",
            "$value": "{ob.p.letterSpacing_px.neg_100}",
            "$description": "For font size 3x for a more compact visual appearance.\nHas to be convertered to em!"
          },
          "narrow": {
            "$type": "letterSpacing",
            "$value": "{ob.p.letterSpacing_px.neg_50}",
            "$description": "For font sizes 2xl for a more compact visual appearance.\nHas to be convertered to em!\n"
          },
          "normal": {
            "$type": "letterSpacing",
            "$value": "{ob.p.letterSpacing_px.0}",
            "$description": "Default letter spacing for body and labels.\nHas to be convertered to em!"
          },
          "wide": {
            "$type": "letterSpacing",
            "$value": "{ob.p.letterSpacing_px.50}",
            "$description": "For buttons for a more confortable visual appearance\nHas to be convertered to em!"
          }
        }
      }
    }
  }
}