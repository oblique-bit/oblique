{
  "ob": {
    "h": {
      "link": {
        "spacing": {
          "gap": {
            "$type": "spacing",
            "$value": "{ob.s.dimension.static.component_size.micro.sm.px}",
            "$description": "Gap between icon and text in the text link."
          }
        },
        "motion": {
          "duration": {
            "$type": "duration",
            "$value": "{ob.s.motion.duration.micro}",
            "$description": "Link transition duration for hover/focus states"
          },
          "easing": {
            "$type": "cubicBezier",
            "$value": "{ob.s.motion.easing.standard}",
            "$description": "Link transition easing"
          }
        },
        "asset": {
          "type": {
            "internal": {
              "$type": "asset",
              "$value": "none",
              "$description": "No icon on internal links (except in lists - defined in another token). This token is informative only; developers should not use tokens for icon. "
            },
            "internal_list": {
              "$type": "asset",
              "$value": "arrow-right",
              "$description": "Icon displayed for internal links (list only). This token is informative only; developers should not use tokens for icon. "
            },
            "external": {
              "$type": "asset",
              "$value": "external",
              "$description": "Icon displayed for all external links. This token is informative only; developers should not use tokens for icon. "
            },
            "phone": {
              "$type": "asset",
              "$value": "phone",
              "$description": "Icon displayed for all phone number links. This token is informative only; developers should not use tokens for icon. "
            },
            "mail": {
              "$type": "asset",
              "$value": "envelope",
              "$description": "Icon displayed for email links. This token is informative only; developers should not use tokens for icon. "
            }
          }
        },
        "color": {
          "default": {
            "$type": "color",
            "$value": "{ob.s3.color.interaction.state.fg.enabled.inversity_normal}",
            "$description": "Default color of unvisited links."
          },
          "visited": {
            "$type": "color",
            "$value": "{ob.s3.color.interaction.state.fg.visited.inversity_normal}",
            "$description": "Color of links the user has already visited."
          },
          "hover": {
            "$type": "color",
            "$value": "{ob.s3.color.interaction.state.fg.hover.inversity_normal}",
            "$description": "Color of link when hovered with the mouse."
          },
          "active": {
            "$type": "color",
            "$value": "{ob.s3.color.interaction.state.fg.pressed.inversity_normal}",
            "$description": "Color of link during click interaction."
          }
        },
        "enabled": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.static.text_decoration.link.emphasis_high}"
          },
          "$description": "a:link"
        },
        "hover": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.static.text_decoration.link.emphasis_high}"
          },
          "$description": "a:hover"
        },
        "focus": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.static.text_decoration.link.emphasis_low}"
          },
          "$description": "a:focus. Displays already a border. No need for additional visual emphasis."
        },
        "active": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.static.text_decoration.link.emphasis_high}"
          },
          "$description": "a:active"
        }
      }
    }
  }
}