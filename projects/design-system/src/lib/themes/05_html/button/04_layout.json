{
  "ob": {
    "h": {
      "button": {
        "label_icon": {
          "spacing": {
            "padding": {
              "vertical": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.component_size.element.sm.rem}"
              },
              "horizontal": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.component_size.element.xl.rem}"
              }
            },
            "gap": {
              "$type": "spacing",
              "$value": "{ob.s.dimension.dynamic.component_size.element.sm.rem}"
            }
          },
          "container": {
            "size": {
              "min_height": {
                "$type": "sizing",
                "$value": "{ob.s.dimension.dynamic.component_size.spacing.md.rem}"
              }
            }
          },
          "icon_size": {
            "$type": "sizing",
            "$value": "{ob.s.dimension.static.component_size.spacing.md.rem}",
            "$description": "Standard proportional size of the icon inside the label_icon button subcomponent."
          }
        },
        "icon_only": {
          "spacing": {
            "padding": {
              "vertical": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.component_size.element.sm.rem}"
              },
              "horizontal": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.component_size.element.sm.rem}"
              }
            }
          }
        },
        "remove": {
          "spacing": {
            "padding": {
              "vertical": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.static.component_size.none.rem}",
                "$description": "Remove button requires zero padding."
              },
              "horizontal": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.static.component_size.none.rem}",
                "$description": "Remove button requires zero padding."
              }
            }
          }
        },
        "border_radius": {
          "$type": "borderRadius",
          "$value": "{ob.s.border_radius.sm}",
          "$description": "Standard border radius for button-aug components - consistent across all sizes"
        },
        "border_width": {
          "$type": "borderWidth",
          "$value": "{ob.s.border_width.xs}",
          "$description": "Tinest border width possible for buttons for aesthetics and functionality: differentiation between secondary and tertiary. Static token, not reacting on changing component-size modes."
        },
        "icon": {
          "size": {
            "$type": "sizing",
            "$value": "{ob.s.dimension.static.component_size.spacing.md.rem}"
          }
        }
      }
    }
  }
}