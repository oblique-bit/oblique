{
  "ob": {
    "c": {
      "tag": {
        "container": {
          "spacing": {
            "gap": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.md}",
              "$description": "Spacing between single tags "
            }
          }
        },
        "spacing": {
          "padding": {
            "vertical": {
              "sm": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.none}",
                "$description": "Top and bottom padding – for small size tags."
              },
              "md": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.xs}",
                "$description": "Top and bottom padding  – for medium size tags."
              },
              "lg": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.lg}",
                "$description": "Top and bottom padding  – for large size tags."
              }
            },
            "horizontal": {
              "sm": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.md}",
                "$description": "Left and right padding – for small size tags."
              },
              "md": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.lg}",
                "$description": "Left and right padding  – for medium size tags."
              },
              "lg": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.2xl}",
                "$description": "Left and right padding – for large size tags."
              }
            }
          },
          "gap": {
            "sm": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.sm}",
              "$description": "Horizontal gap between icon and text label – for small size tags."
            },
            "md": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.md}",
              "$description": "Horizontal gap between icon and text label  – for medium size tags."
            },
            "lg": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.lg}",
              "$description": "Horizontal gap between icon and text label – for large size tags"
            }
          }
        },
        "border-radius": {
          "default": {
            "$type": "borderRadius",
            "$value": "{ob.s.radii.round}",
            "$description": "Standard tags in Oblique."
          }
        },
        "icon": {
          "size": {
            "sm": {
              "$type": "sizing",
              "$value": "{ob.s.icon.size.xs}"
            },
            "md": {
              "$type": "sizing",
              "$value": "{ob.s.icon.size.xs}"
            },
            "lg": {
              "$type": "sizing",
              "$value": "{ob.s.icon.size.sm}"
            }
          }
        },
        "surface": {
          "min-height": {
            "sm": {
              "$type": "sizing",
              "$value": "{ob.p.size.250}"
            },
            "md": {
              "$type": "sizing",
              "$value": "{ob.p.size.300}"
            },
            "lg": {
              "$type": "sizing",
              "$value": "36"
            }
          }
        },
        "typography": {
          "size": {
            "sm": {
              "$type": "typography",
              "$value": "{ob.s.typography.type-scale.xs.normal}"
            },
            "md": {
              "$type": "typography",
              "$value": "{ob.s.typography.type-scale.sm.normal}"
            },
            "lg": {
              "$type": "typography",
              "$value": "{ob.s.typography.type-scale.md.normal}"
            }
          }
        },
        "color": {
          "fg": {
            "enabled": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.fg.contrast-medium}"
            },
            "hover": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.fg.contrast-medium}"
            },
            "focus": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.fg.contrast-medium}"
            },
            "active": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.fg.contrast-medium-inverse}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{ob.s.color.interaction.state.fg.disabled}"
            }
          },
          "bg": {
            "enabled": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.bg.contrast-medium}"
            },
            "hover": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.bg.contrast-high}"
            },
            "focus": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.bg.contrast-medium}"
            },
            "active": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.bg.contrast-medium-inverse}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{ob.s.color.neutral.bg.contrast-medium}"
            }
          }
        }
      }
    }
  }
}