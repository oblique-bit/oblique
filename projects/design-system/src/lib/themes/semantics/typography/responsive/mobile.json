{
  "ob": {
    "s": {
      "fontSize": {
        "xs": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.100}"
        },
        "sm": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.200}"
        },
        "md": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.300} + 1",
          "$description": "slightly larger front size for mobile for improved readability"
        },
        "lg": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.400}"
        },
        "xl": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.500}"
        },
        "2xl": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.500}"
        },
        "3xl": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.500}"
        },
        "4xl": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.500}"
        },
        "_base": {
          "$type": "fontSizes",
          "$value": "{ob.p.fontSizeUnitless.300}"
        }
      },
      "lineHeight": {
        "xs": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.300}"
        },
        "sm": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.400}"
        },
        "md": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.500}"
        },
        "lg": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.600}"
        },
        "xl": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.700}"
        },
        "2xl": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.700}"
        },
        "3xl": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.700}"
        },
        "4xl": {
          "$type": "lineHeights",
          "$value": "{ob.p.lineHeight.700}"
        }
      },
      "spacing": {
        "heading": {
          "top": {
            "$type": "spacing",
            "$value": "{ob.s.spacing.none}",
            "$description": "Standard spacing above H1 is none to use vertical space."
          },
          "top-after-p": {
            "$type": "spacing",
            "$value": "{ob.s.spacing.6xl}",
            "$description": "SPecial spacing above H1 after a paragraph"
          },
          "bottom": {
            "$type": "spacing",
            "$value": "{ob.s.spacing.2xl}"
          }
        },
        "paragraph": {
          "bottom": {
            "$type": "spacing",
            "$value": "{ob.s.spacing.2xl}"
          }
        }
      },
      "paragraphSpacing": {
        "xs": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.xs} / 2"
        },
        "sm": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.sm} / 2"
        },
        "md": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.md} / 2"
        },
        "lg": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.lg} / 2"
        },
        "xl": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.xl} / 2"
        },
        "2xl": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.2xl} / 2"
        },
        "3xl": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.3xl} / 2"
        },
        "4xl": {
          "$type": "paragraphSpacing",
          "$value": "{ob.s.lineHeight.4xl} / 2"
        }
      }
    }
  }
}