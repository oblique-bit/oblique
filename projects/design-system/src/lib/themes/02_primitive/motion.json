{
  "ob": {
    "p": {
      "motion": {
        "duration": {
          "instant": {
            "$type": "duration",
            "$value": "0ms",
            "$description": "For immediate state changes with no transition"
          },
          "fast": {
            "$type": "duration",
            "$value": "100ms",
            "$description": "For micro-interactions like button hovers"
          },
          "medium": {
            "$type": "duration",
            "$value": "200ms",
            "$description": "For tooltip appearances and simple transitions"
          },
          "slow": {
            "$type": "duration",
            "$value": "300ms",
            "$description": "For modal appearances and complex transitions"
          },
          "slower": {
            "$type": "duration",
            "$value": "500ms",
            "$description": "For page transitions and major layout changes"
          }
        },
        "easing": {
          "linear": {
            "$type": "cubicBezier",
            "$value": "cubic-bezier(0, 0, 1, 1)",
            "$description": "Linear motion with no acceleration"
          },
          "ease_out": {
            "$type": "cubicBezier",
            "$value": "cubic-bezier(0, 0, 0.2, 1)",
            "$description": "Starts fast, ends slow - good for entrances"
          },
          "ease_in": {
            "$type": "cubicBezier",
            "$value": "cubic-bezier(0.4, 0, 1, 1)",
            "$description": "Starts slow, ends fast - good for exits"
          },
          "ease_in_out": {
            "$type": "cubicBezier",
            "$value": "cubic-bezier(0.4, 0, 0.2, 1)",
            "$description": "Slow start and end - good for emphasis"
          }
        }
      }
    }
  }
}