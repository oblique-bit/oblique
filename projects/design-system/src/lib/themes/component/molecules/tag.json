{
  "ob": {
    "c": {
      "tag": {
        "container": {
          "spacing": {
            "gap": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.md}",
              "$description": "Spacing between single tags "
            }
          }
        },
        "spacing": {
          "padding": {
            "vertical": {
              "sm": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.none}",
                "$description": "Top and bottom padding – for small size tags."
              },
              "md": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.xs}",
                "$description": "Top and bottom padding  – for medium size tags."
              },
              "lg": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.lg}",
                "$description": "Top and bottom padding  – for large size tags."
              }
            },
            "horizontal": {
              "sm": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.md}",
                "$description": "Left and right padding – for small size tags."
              },
              "md": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.lg}",
                "$description": "Left and right padding  – for medium size tags."
              },
              "lg": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.2xl}",
                "$description": "Left and right padding – for large size tags."
              }
            }
          },
          "gap": {
            "sm": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.sm}",
              "$description": "Horizontal gap between icon and text label – for small size tags."
            },
            "md": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.md}",
              "$description": "Horizontal gap between icon and text label  – for medium size tags."
            },
            "lg": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.lg}",
              "$description": "Horizontal gap between icon and text label – for large size tags"
            }
          }
        },
        "border_radius": {
          "default": {
            "$type": "borderRadius",
            "$value": "{ob.s.radii.round}",
            "$description": "Standard tags in Oblique."
          }
        },
        "icon": {
          "size": {
            "sm": {
              "$type": "sizing",
              "$value": "{ob.s.icon.size.xs}"
            },
            "md": {
              "$type": "sizing",
              "$value": "{ob.s.icon.size.xs}"
            },
            "lg": {
              "$type": "sizing",
              "$value": "{ob.s.icon.size.sm}"
            }
          }
        },
        "surface": {
          "min_height": {
            "sm": {
              "$type": "sizing",
              "$value": "{ob.s.size.stretchy}"
            },
            "md": {
              "$type": "sizing",
              "$value": "{ob.s.size.spacious}"
            },
            "lg": {
              "$type": "sizing",
              "$value": "{ob.s.size.hefty}"
            }
          }
        },
        "typography": {
          "size": {
            "sm": {
              "$type": "typography",
              "$value": "{ob.s.typography.type_scale.xs.normal}"
            },
            "md": {
              "$type": "typography",
              "$value": "{ob.s.typography.type_scale.sm.normal}"
            },
            "lg": {
              "$type": "typography",
              "$value": "{ob.s.typography.type_scale.md.normal}"
            }
          }
        },
        "color": {
          "fg": {
            "enabled": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.fg.contrast_high}"
            },
            "hover": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.fg.contrast_high}"
            },
            "focus": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.fg.contrast_high}"
            },
            "active": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.fg.contrast_medium.inversity_flipped}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{ob.s3.color.interaction.state.fg.disabled.inversity_normal}"
            }
          },
          "bg": {
            "enabled": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.bg.contrast_low.inversity_normal}"
            },
            "hover": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.bg.contrast_high.inversity_normal}"
            },
            "focus": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.bg.contrast_low.inversity_normal}"
            },
            "active": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.bg.contrast_medium.inversity_flipped}"
            },
            "disabled": {
              "$type": "color",
              "$value": "{ob.s3.color.neutral.bg.contrast_low.inversity_normal}"
            }
          }
        }
      }
    }
  }
}