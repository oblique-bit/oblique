{
  "ob": {
    "h": {
      "link": {
        "spacing": {
          "gap": {
            "$type": "spacing",
            "$value": "{ob.s.spacing.md}",
            "$description": "Gap between icon and text in the text link."
          }
        },
        "asset": {
          "type": {
            "internal": {
              "$type": "asset",
              "$value": "none",
              "$description": "No icon on internal links (except in lists - defined in another token). Devselopers do not use this in the code."
            },
            "internal-list": {
              "$type": "asset",
              "$value": "arrow-right",
              "$description": "Icon displayed for internal links (list only). Devselopers do not use this in the code."
            },
            "external": {
              "$type": "asset",
              "$value": "external",
              "$description": "Icon displayed for all external links. Devselopers do not use this in the code."
            },
            "phone": {
              "$type": "asset",
              "$value": "phone",
              "$description": "Icon displayed for all phone number links. Devselopers do not use this in the code."
            },
            "mail": {
              "$type": "asset",
              "$value": "envelope",
              "$description": "Icon displayed for email links. Devselopers do not use this in the code."
            }
          }
        },
        "color": {
          "link": {
            "$type": "color",
            "$value": "{ob.s.color.interaction.state.fg.enabled.inversity-normal}",
            "$description": "Default color of unvisited links."
          },
          "visited": {
            "$type": "color",
            "$value": "{ob.s.color.interaction.state.fg.visited.inversity-normal}",
            "$description": "Color of links the user has already visited."
          },
          "hover": {
            "$type": "color",
            "$value": "{ob.s.color.interaction.state.fg.hover.inversity-normal}",
            "$description": "Color of link when hovered with the mouse."
          },
          "active": {
            "$type": "color",
            "$value": "{ob.s.color.interaction.state.fg.pressed.inversity-normal}",
            "$description": "Color of link during click interaction."
          },
          "disabled": {
            "$type": "color",
            "$value": "{ob.s.color.interaction.state.fg.disabled.inversity-normal}",
            "$description": "Color of a visually disabled, non-clickable link."
          }
        },
        "enabled": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.text-decoration.link.emphasis-medium}"
          },
          "$description": "a:link"
        },
        "hover": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.text-decoration.link.emphasis-medium}"
          },
          "$description": "a:hover"
        },
        "focus": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.text-decoration.link.emphasis-low}"
          },
          "$description": "a:focus. Displays already a border. No need for additional visual emphasis."
        },
        "active": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.text-decoration.link.emphasis-medium}"
          },
          "$description": "a:active"
        },
        "disabled": {
          "$type": "typography",
          "$value": {
            "textDecoration": "{ob.s.text-decoration.link.emphasis-low}"
          },
          "$description": "non css standard but required.No need for underline when disabled."
        }
      }
    }
  }
}