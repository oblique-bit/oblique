{
  "ob": {
    "h": {
      "button": {
        "spacing": {
          "with_text": {
            "padding": {
              "vertical": {
                "sm": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.sm}",
                  "$description": "Top and bottom padding – for small size buttons."
                },
                "md": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.md}",
                  "$description": "Top and bottom padding  – for medium size buttons."
                },
                "lg": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.2xl}",
                  "$description": "Top and bottom padding  – for large size buttons."
                }
              },
              "horizontal": {
                "sm": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.xl}",
                  "$description": "Left and right padding – for small size buttons."
                },
                "md": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.3xl}",
                  "$description": "Left and right padding  – for medium size buttons."
                },
                "lg": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.4xl}",
                  "$description": "Left and right padding – for large size buttons."
                }
              }
            },
            "gap": {
              "sm": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.sm}",
                "$description": "Horizontal gap between icon and text label – for small size buttons."
              },
              "md": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.md}",
                "$description": "Horizontal gap between icon and text label  – for medium size buttons."
              },
              "lg": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.lg}",
                "$description": "Horizontal gap between icon and text label – for large size buttons"
              }
            }
          },
          "icon_only": {
            "padding": {
              "vertical": {
                "xs": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.none}",
                  "$description": "No spacing needed inside the mini close button. Use case: inside tag component."
                },
                "sm": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.md}",
                  "$description": "Forces the icon_only button to be round and the same height as a small with_text button."
                },
                "md": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.lg}",
                  "$description": "Forces the icon_only button to be round and the same height as a medium with_text button."
                },
                "lg": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.2xl}",
                  "$description": "Forces the icon_only button to be round and the same height as a large with_text button."
                }
              },
              "horizontal": {
                "xs": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.none}",
                  "$description": "No spacing needed inside the close button."
                },
                "sm": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.md}",
                  "$description": "Forces the icon_only button to be a circle."
                },
                "md": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.lg}",
                  "$description": "Forces the icon_only button to be a circle."
                },
                "lg": {
                  "$type": "spacing",
                  "$value": "{ob.s.spacing.2xl}",
                  "$description": "Forces the icon_only button to be a circle."
                }
              }
            },
            "gap": {
              "sm": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.sm}",
                "$description": "Not required in the code. Used only for simpler maintaining reasons in Figma."
              },
              "md": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.md}",
                "$description": "Not required in the code. Used only for simpler maintaining reasons in Figma."
              },
              "lg": {
                "$type": "spacing",
                "$value": "{ob.s.spacing.lg}",
                "$description": "Not required in the code. Used only for simpler maintaining reasons in Figma."
              }
            }
          }
        },
        "border_radius": {
          "default": {
            "$type": "borderRadius",
            "$value": "{ob.s.radii.sm}",
            "$description": "Standard buttons in Oblique."
          }
        },
        "border_width": {
          "$type": "borderWidth",
          "$value": "{ob.s.border_width.xs}",
          "$description": "Default border width for buttons (additional hover border not included)"
        },
        "box_shadow": {
          "enabled": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow needed on enabled state of button."
          },
          "hover": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.xl}",
            "$description": "A shadow needed for elevation effect on hover state of button."
          },
          "focus": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow needed on focus state of button."
          },
          "pressed": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow needed on pressed state of button."
          },
          "disabled": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow needed on disabled state of button."
          }
        },
        "icon": {
          "size": {
            "sm": {
              "$type": "sizing",
              "$value": "{ob.c.icon_holder.size.xs}"
            },
            "md": {
              "$type": "sizing",
              "$value": "{ob.c.icon_holder.size.standard}"
            },
            "lg": {
              "$type": "sizing",
              "$value": "{ob.c.icon_holder.standard.size.md}"
            }
          }
        },
        "icon_only": {
          "surface": {
            "size": {
              "xs": {
                "$type": "sizing",
                "$value": "{ob.s.size.none}",
                "$description": "Used as close button in ds tag component. Tertiary only, never has badge, emphasis muted only."
              },
              "sm": {
                "$type": "sizing",
                "$value": "{ob.s.size.spacious}"
              },
              "md": {
                "$type": "sizing",
                "$value": "{ob.s.size.hefty}"
              },
              "lg": {
                "$type": "sizing",
                "$value": "{ob.s.size.ultra}"
              }
            }
          }
        },
        "typography": {
          "text_label": {
            "$type": "typography",
            "$value": "{ob.s.typography.type_scale.sm.normal}",
            "$description": "Single font style and size for all button sizes and types for consistancy."
          }
        },
        "badge": {
          "defaultsize": {
            "$type": "text",
            "$value": "lg"
          }
        },
        "close": {
          "assets": {
            "xmark_circle": {
              "icon": {
                "$type": "asset",
                "$value": "xmark_circle.svg",
                "$description": "Close button icon for molucules such as Pills, Tags."
              }
            },
            "xmark": {
              "icon": {
                "$type": "asset",
                "$value": "xmark.svg",
                "$description": "Close button icon for molucules such as Pills, Tags."
              }
            }
          },
          "icon": {
            "xmark_circle": {
              "icon": {
                "$type": "text",
                "$value": "xmark_circle.svg",
                "$description": "Close button icon for molucules such as Pills, Tags."
              }
            },
            "xmark": {
              "icon": {
                "$type": "text",
                "$value": "xmark.svg",
                "$description": "Close button icon for molucules such as Pills, Tags."
              }
            }
          }
        }
      }
    }
  }
}