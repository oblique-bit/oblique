{
  "ob": {
    "h": {
      "button_aug": {
        "container": {
          "spacing": {
            "gap": {
              "$type": "spacing",
              "$value": "{ob.s.spacing.3xl}",
              "$description": "Button gap for desktop viewport - wider spacing for mouse interaction"
            }
          }
        },
        "typography": {
          "text_label": {
            "font_size": {
              "$extensions": {
                "com.tokens-studio": {
                  "mode": {
                    "size-sm": "{ob.s.fontSize.xs}",
                    "size-md": "{ob.s.fontSize.sm}",
                    "size-lg": "{ob.s.fontSize.md}"
                  }
                }
              },
              "$type": "fontSizes",
              "$value": "{ob.s.fontSize.sm}",
              "$description": "Desktop font size for button-aug text labels"
            }
          }
        },
        "touch_target": {
          "min_size": {
            "$type": "dimension",
            "$value": "44px",
            "$description": "Standard desktop touch target minimum size"
          }
        }
      }
    }
  }
}