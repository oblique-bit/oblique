{
  "ob": {
    "h": {
      "button_aug": {
        "$description": {
          "$type": "other",
          "$value": "Size-responsive button tokens - Uses semantic dimension tokens that scale with size multipliers"
        },
        "text_icons": {
          "spacing": {
            "padding": {
              "vertical": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.element.sm.px}"
              },
              "horizontal": {
                "$type": "spacing", 
                "$value": "{ob.s.dimension.dynamic.element.md.px}"
              }
            },
            "gap": {
              "$type": "spacing",
              "$value": "{ob.s.dimension.dynamic.element.sm.px}"
            }
          },
          "container": {
            "size": {
              "min_height": {
                "$type": "sizing",
                "$value": "{ob.s.dimension.dynamic.container.xs.px}"
              }
            }
          }
        },
        "icon_only": {
          "spacing": {
            "padding": {
              "vertical": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.element.sm.px}"
              },
              "horizontal": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.dynamic.element.sm.px}"
              }
            }
          }
        },
        "typography": {
          "text_label": {
            "font_size": {
              "$type": "fontSizes",
              "$value": "{ob.p.fontSizeUnitless.400} * {ob.g.multiplier.typography}",
              "$description": "Font size that scales with typography multiplier"
            },
            "line_height": {
              "$type": "lineHeights", 
              "$value": "{ob.p.lineHeightUnitless.400} * {ob.g.multiplier.typography}",
              "$description": "Line height that scales with typography multiplier"
            },
            "letter_spacing": {
              "$type": "letterSpacing",
              "$value": "{ob.p.letter_spacing.normal}",
              "$description": "Letter spacing for button text labels"
            }
          }
        },
        "remove": {
          "spacing": {
            "padding": {
              "vertical": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.static.none.px}",
                "$description": "Remove button requires zero padding."
              },
              "horizontal": {
                "$type": "spacing",
                "$value": "{ob.s.dimension.static.none.px}",
                "$description": "Remove button requires zero padding."
              }
            }
          }
        }
      }
    }
  }
}