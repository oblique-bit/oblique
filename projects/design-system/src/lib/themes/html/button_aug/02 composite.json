{
  "ob": {
    "h": {
      "button_aug": {
        "$description": {
          "$type": "other",
          "$value": "Composite tokens - Source only, not exposed in Figma. Purpose: boosting maintainability in Figma by providing pre-composed token combinations."
        },
        "text_icons": {
          "composite": {
            "primary": {
              "inversity_normal": {
                "enabled": {
                  "button_surface": {
                    "$type": "composition",
                    "$value": {
                      "fill": "{ob.h.button_aug.color.bg.primary.inversity_normal.enabled}",
                      "borderColor": "{ob.h.button_aug.color.border.primary.inversity_normal.enabled}"
                    }
                  },
                  "fg": {
                    "$type": "composition",
                    "$value": {
                      "fill": "{ob.h.button_aug.color.fg.primary.inversity_normal.enabled}"
                    }
                  }
                },
                "hover": {
                  "button_surface": {
                    "$type": "composition",
                    "$value": {
                      "fill": "{ob.h.button_aug.color.bg.primary.inversity_normal.hover}",
                      "borderColor": "{ob.h.button_aug.color.border.primary.inversity_normal.hover}"
                    }
                  },
                  "fg": {
                    "$type": "composition",
                    "$value": {
                      "fill": "{ob.h.button_aug.color.fg.primary.inversity_normal.hover}"
                    }
                  }
                },
                "focus": {
                  "button_surface": {
                    "$type": "composition",
                    "$value": {
                      "fill": "{ob.h.button_aug.color.bg.primary.inversity_normal.focus}",
                      "borderColor": "{ob.h.button_aug.color.border.primary.inversity_normal.focus}"
                    }
                  },
                  "fg": {
                    "$type": "composition",
                    "$value": {
                      "fill": "{ob.h.button_aug.color.fg.primary.inversity_normal.hover}"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}