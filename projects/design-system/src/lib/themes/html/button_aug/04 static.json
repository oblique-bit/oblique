{
  "ob": {
    "h": {
      "button_aug": {
        "$description": {
          "$type": "other",
          "$value": "Button-aug static tokens - size-specific tokens are in separate size files (xs, sm, md, lg)"
        },
        "container": {
          "spacing": {
            "gap": {
              "$extensions": {
                "com.tokens-studio": {
                  "mode": {
                    "container-wide": "{ob.s.spacing.3xl}",
                    "container-narrow": "{ob.s.spacing.lg}"
                  }
                }
              },
              "$type": "spacing",
              "$value": "{ob.s.spacing.3xl}",
              "$description": "Gap between buttons in container - adapts to container=wide|narrow context"
            }
          }
        },
        "border_radius": {
          "default": {
            "$type": "borderRadius",
            "$value": "{ob.s.radii.sm}",
            "$description": "Standard border radius for button-aug components - consistent across all sizes"
          }
        },
        "box_shadow": {
          "enabled": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow on enabled state"
          },
          "hover": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.xl}",
            "$description": "Elevation effect on hover"
          },
          "focus": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow on focus state"
          },
          "pressed": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow on pressed state"
          },
          "disabled": {
            "$type": "boxShadow",
            "$value": "{ob.s.shadow.none}",
            "$description": "No shadow on disabled state"
          }
        },
        "typography": {
          "text_label": {
            "font_family": {
              "$type": "fontFamilies",
              "$value": "{ob.s.font_family.body}",
              "$description": "Font family for button text labels - consistent across all sizes"
            },
            "font_weight": {
              "$type": "fontWeights",
              "$value": "{ob.s.font_weight.medium}",
              "$description": "Font weight for button text labels - consistent across all sizes"
            },
            "text_decoration": {
              "$type": "textDecoration",
              "$value": "none",
              "$description": "No text decoration for button labels"
            },
            "text_transform": {
              "$type": "textCase",
              "$value": "none",
              "$description": "No text transformation for button labels"
            },
            "text_align": {
              "$type": "textAlign",
              "$value": "center",
              "$description": "Center alignment for button text labels. Note: Not supported by Figma variables - set manually in Figma components."
            }
          }
        }
      },
      "button-aug": {
        "border_width": {
          "$type": "borderWidth",
          "$value": "{ob.s.border_width.xs}",
          "$description": "Default border width for button-aug - consistent across all sizes"
        }
      }
    }
  }
}