{
  "ob": {
    "c": {
      "button-aug": {
        "$description": "Composite button-aug tokens for essential variants - optimized for Token Studio mode-based mapping to Figma layers",
        
        "primary": {
          "$description": "Primary button-aug variants with mode-based sizing",
          
          "enabled": {
            "$description": "Primary button-aug enabled state - complete styling package with size modes",
            "normal": {
              "$type": "composite",
              "$value": {
                "typography": {
                  "$type": "typography", 
                  "$value": "{ob.h.button-aug.typography.text_label}"
                },
                "backgroundColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.bg.primary.inversity_flipped.enabled}"
                },
                "textColor": {
                  "$type": "color", 
                  "$value": "{ob.h.button-aug.color.fg.primary.inversity_flipped.enabled}"
                },
                "borderColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.border.primary.inversity_flipped.enabled}"
                },
                "borderWidth": {
                  "$type": "borderWidth",
                  "$value": "{ob.h.button-aug.border_width}"
                },
                "borderRadius": {
                  "$type": "borderRadius",
                  "$value": "{ob.h.button-aug.border_radius.default}"
                },
                "paddingVertical": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.spacing.with_text.padding.vertical}"
                },
                "paddingHorizontal": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.spacing.with_text.padding.horizontal}"
                },
                "boxShadow": {
                  "$type": "shadow",
                  "$value": "{ob.h.button-aug.box_shadow.enabled}"
                },
                "iconSize": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.icon.size}"
                },
                "iconSpacing": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.spacing.with_text.gap}"
                }
              },
              "$extensions": {
                "com.tokens-studio": {
                  "figma": {
                    "variant": "type=primary, state=enabled, inversity=flipped"
                  },
                  "mode": {
                    "size-sm": "Small primary button",
                    "size-md": "Medium primary button", 
                    "size-lg": "Large primary button"
                  }
                }
              }
            }
          },
          
          "hover": {
            "$description": "Primary button-aug hover state",
            "normal": {
              "$type": "composite",
              "$value": {
                "backgroundColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.bg.primary.inversity_flipped.hover}"
                },
                "textColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.fg.primary.inversity_flipped.hover}"
                },
                "borderColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.border.primary.inversity_flipped.hover}"
                },
                "boxShadow": {
                  "$type": "shadow",
                  "$value": "{ob.h.button-aug.box_shadow.hover}"
                }
              },
              "$extensions": {
                "com.tokens-studio": {
                  "figma": {
                    "variant": "type=primary, state=hover, inversity=flipped"
                  }
                }
              }
            }
          }
        },
        
        "secondary": {
          "$description": "Secondary button-aug variants with mode-based sizing",
          
          "enabled": {
            "$description": "Secondary button-aug enabled state",
            "normal": {
              "$type": "composite",
              "$value": {
                "typography": {
                  "$type": "typography",
                  "$value": "{ob.h.button-aug.typography.text_label}"
                },
                "backgroundColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.bg.secondary.inversity_normal.enabled}"
                },
                "textColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.fg.secondary.inversity_normal.enabled}"
                },
                "borderColor": {
                  "$type": "color",
                  "$value": "{ob.h.button-aug.color.border.secondary.inversity_normal.enabled}"
                },
                "borderWidth": {
                  "$type": "borderWidth",
                  "$value": "{ob.h.button-aug.border_width}"
                },
                "borderRadius": {
                  "$type": "borderRadius",
                  "$value": "{ob.h.button-aug.border_radius.default}"
                },
                "paddingVertical": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.spacing.with_text.padding.vertical}"
                },
                "paddingHorizontal": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.spacing.with_text.padding.horizontal}"
                },
                "boxShadow": {
                  "$type": "shadow",
                  "$value": "{ob.h.button-aug.box_shadow.enabled}"
                },
                "iconSize": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.icon.size}"
                },
                "iconSpacing": {
                  "$type": "dimension",
                  "$value": "{ob.h.button-aug.spacing.with_text.gap}"
                }
              },
              "$extensions": {
                "com.tokens-studio": {
                  "figma": {
                    "variant": "type=secondary, state=enabled, inversity=normal"
                  },
                  "mode": {
                    "size-sm": "Small secondary button",
                    "size-md": "Medium secondary button",
                    "size-lg": "Large secondary button"
                  }
                }
              }
            }
          }
        },
        
        "container": {
          "$description": "Container composite tokens for button-aug groups",
          
          "wide": {
            "$description": "Wide container button group",
            "normal": {
              "$type": "composite",
              "$value": {
                "flexDirection": {
                  "$type": "text",
                  "$value": "{ob.h.button-aug.container.responsive.wide.layout.direction}"
                },
                "alignItems": {
                  "$type": "text",
                  "$value": "{ob.h.button-aug.container.responsive.wide.layout.align_items}"
                },
                "gap": {
                  "$type": "spacing",
                  "$value": "{ob.h.button-aug.container.responsive.wide.spacing.gap}"
                },
                "flexWrap": {
                  "$type": "text",
                  "$value": "{ob.h.button-aug.container.responsive.wide.layout.flex_wrap}"
                }
              },
              "$extensions": {
                "com.tokens-studio": {
                  "figma": {
                    "variant": "container=wide"
                  }
                }
              }
            }
          },
          
          "narrow": {
            "$description": "Narrow container button group",
            "normal": {
              "$type": "composite",
              "$value": {
                "flexDirection": {
                  "$type": "text",
                  "$value": "{ob.h.button-aug.container.responsive.narrow.layout.direction}"
                },
                "alignItems": {
                  "$type": "text",
                  "$value": "{ob.h.button-aug.container.responsive.narrow.layout.align_items}"
                },
                "gap": {
                  "$type": "spacing",
                  "$value": "{ob.h.button-aug.container.responsive.narrow.spacing.gap}"
                }
              },
              "$extensions": {
                "com.tokens-studio": {
                  "figma": {
                    "variant": "container=narrow"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
