{
  "$schema": "http://json-schema.org/schema",
  "$id": "hello-world-schematic",
  "title": "Hello World Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the file",
      "default": "hello-world.txt"
    },
    "content": {
      "type": "string",
      "description": "Content of the file",
      "default": "Hello world"
    }
  },
  "required": ["name"]
}
